{
  "name": "angular-practice",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:stats": "ng b --prod=true --statsJson=true",
    "run:analyzer": "webpack-bundle-analyzer dist/$npm_package_name/stats-es2015.json -p 5000",
    "analyze": "run-s build:stats run:analyzer"
  },
  "private": true,
  "dependencies": {
